DROP TABLE goals;
DROP TABLE users;

CREATE TABLE goals (
    goalsid SERIAL PRIMARY KEY NOT NULL,
    goalname varchar(20),
    description varchar(80),
    startdate VARCHAR(11),
    enddate VARCHAR(11)
);

CREATE TABLE users (
  id SERIAL,
  auth0_id TEXT,
  email TEXT,
  pictureUrl TEXT,
  name TEXT
);


SELECT * FROM goals;
SELECT * FROM users;